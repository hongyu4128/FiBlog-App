{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/fabric": "^2.3.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.2",
    "antd": "^4.9.4",
    "classnames": "^2.2.6",
    "eslint": "^7.15.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.3.2",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
